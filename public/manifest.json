{
  "manifest_version": 3,
  "name": "chromate",
  "version": "1.0",
  "description": "",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "16": "images/logo_white_16.png",
    "32": "images/logo_white_32.png",
    "48": "images/logo_white_48.png",
    "128": "images/logo_white_128.png"
  }
}
